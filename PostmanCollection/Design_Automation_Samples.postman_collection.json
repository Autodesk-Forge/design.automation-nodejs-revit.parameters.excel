							{
								"key": "Authorization",
								"value": "Bearer {{dasApiToken}}"
							}
						],
						"body": {
							"mode": "raw",
							"raw": "\r\n\r\n\r\n\r\n{\r\n\t\"id\": \"{{activityName}}\",\r\n\t\"commandLine\": [ \"$(engine.path)\\\\\\\\revitcoreconsole.exe /i $(args[inputFile].path) /al $(appbundles[{{appBundleName}}].path)\" ],\r\n    \"parameters\": {\r\n        \"inputFile\": {\r\n            \"verb\": \"get\",\r\n            \"description\": \"input file\",\r\n            \"required\": true,\r\n            \"localName\": \"$(inputFile)\"\r\n        },\r\n        \"inputJson\": {\r\n            \"verb\": \"get\",\r\n            \"description\": \"input Json parameters\",\r\n            \"localName\": \"params.json\"\r\n        },\r\n        \"inputXls\": {\r\n            \"verb\": \"get\",\r\n            \"description\": \"input excel file\",\r\n            \"localName\": \"input.xls\"\r\n        },\r\n        \"outputRvt\": {\r\n            \"verb\": \"put\",\r\n            \"description\": \"output Rvt file\",\r\n            \"localName\": \"result.rvt\"\r\n        },\r\n        \"outputXls\": {\r\n            \"verb\": \"put\",\r\n            \"description\": \"output excel file\",\r\n            \"localName\": \"result.xls\"\r\n        }\r\n    },\r\n\t\"engine\": \"Autodesk.Revit+2019\",\r\n\t\"appbundles\": [ \"{{dasNickName}}.{{appBundleName}}+{{appBundleAlias}}\" ],\r\n\t\"description\": \"Export & Import Parameter values to Excel Activity.\"\r\n}"
						},
